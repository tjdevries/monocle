(version 7.0.0)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends dune ocaml extlib base64 cudf ocamlgraph re stdlib-shims)

